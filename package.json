{
  "name": "lucai-chat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:lucai-css": "postcss styles/vendor/lucai/index.css -o public/vendor/lucai.scoped.css --config postcss.lucai.cjs",
    "predev": "npm run build:lucai-css",
    "prebuild": "npm run build:lucai-css",
    "dev": "cross-env NEXT_DISABLE_TURBOPACK=1 next dev",
    "build": "cross-env NEXT_DISABLE_TURBOPACK=1 next build",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "lightningcss": "^1.30.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "postcss-prefix-selector": "^2.1.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
